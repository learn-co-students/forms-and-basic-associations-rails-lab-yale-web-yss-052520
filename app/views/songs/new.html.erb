<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :song_title %>
  <%= f.text_field :title %>
  <br />

   <%= f.label :song_artist_name %>
  <%= f.text_field :artist_name %>
  <br />

   <%= f.label :genre %>
  <%= f.collection_select :genre_id, @genres, :id, :name %>
  <br />

